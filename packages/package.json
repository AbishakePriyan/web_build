{
  "name": "@studio-freight/lenis-test",
  "version": "0.0.3",
  "author": "darkroom.engineering",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darkroomengineering/lenis.git"
  },
  "bugs": {
    "url": "https://github.com/darkroomengineering/lenis/issues"
  },
  "files": [
    "core/dist",
    "react/dist"
  ],
  "scripts": {
    "build:core": "npm run build --prefix ./core",
    "build:react": "npm run build --prefix ./react",
    "build": "npm-run-all --parallel build:core build:react"
  },
  "sideEffects": false,
  "source": "./core/src/index.js",
  "main": "./core/dist/lenis.umd.js",
  "unpkg": "./core/dist/lenis.min.js",
  "module": "./core/dist/lenis.mjs",
  "types": "./core/dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./core/dist/types/index.d.ts",
      "default": "./core/dist/lenis.mjs"
    },
    "./react": {
      "types": "./react/dist/types/index.d.ts",
      "default": "./react/dist/lenis-react.mjs"
    }
  },
  "dependencies": {
    "@studio-freight/hamo": "^0.6.28",
    "@studio-freight/lenis": "^1.0.40",
    "@types/react": "^18.0.0",
    "clsx": "^2.0.0",
    "react": "^18.0.0",
    "zustand": "^4.4.7"
  },
  "peerDependencies": {
    "react": "^17 || ^18",
    "react-dom": "^17 || ^18"
  }
}
